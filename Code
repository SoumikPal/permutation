#include <stdio.h>
#include <string.h>
int perm(char q[],int k,int n);
int main()
{
   char a[50];
   printf("Enter a number whose permutation you need");
   gets(a);
   int p=strlen(a);
   perm(a,0,p);
}
int perm(char q[],int k,int n)
{
    if(k==n-1)
    {
        puts(q);
        printf("\n");
        return 0;
    }
    else
    {
        int i;
        for(i=k;i<n;i++)
        {
            char r=q[i];
            q[i]=q[k];
            q[k]=r;
            perm(q,k+1,n);
            char l=q[i];
            q[i]=q[k];
            q[k]=l;

        }
    }
}

